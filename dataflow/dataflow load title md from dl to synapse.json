{
	"name": "dataflow load title md from dl to synapse",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_dl_title_md",
						"type": "DatasetReference"
					},
					"name": "sourcedltitlemd"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_synapse_title_md",
						"type": "DatasetReference"
					},
					"name": "sinksyanpsetitlemd"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcedltitlemd",
				"sourcedltitlemd sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sinksyanpsetitlemd"
			]
		}
	}
}