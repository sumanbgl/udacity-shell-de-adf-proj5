{
	"name": "dataflow load agency md from dl to synapse",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_dl_agency_md",
						"type": "DatasetReference"
					},
					"name": "sourcedlagencymd"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_synapse_agency_md",
						"type": "DatasetReference"
					},
					"name": "sinksynapseagencymd"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcedlagencymd",
				"sourcedlagencymd sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sinksynapseagencymd"
			]
		}
	}
}